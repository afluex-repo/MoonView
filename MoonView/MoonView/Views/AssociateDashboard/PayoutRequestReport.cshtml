@model MoonView.Models.AssociateBooking

@{
    ViewBag.Title = "PayoutRequestReport";
    Layout = "~/Views/Shared/AssociateLayout.cshtml";
}

@using (Html.BeginForm("PayoutRequestReport", "AssociateDashboard", FormMethod.Post, new { @id = "form1" }))
{


    <div class="page-header">
        <div class="page-block">
            <div class="row align-items-center">
                <div class="col-md-8">
                    <div class="page-header-title">
                        <h4 class="m-b-10">Payout Request Report</h4>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">
                <div class="page-body">
                    <div class="row">
                        <div class="col-md-12">

                            <!-- HTML5 Export Buttons table start -->
                            <div class="card">
                                <div class="card-header table-card-header">
                                    <p style="color:red">@TempData["Request"]</p>
                                </div>
                                <div class="card-block">
                                    <div class="row">

                                        <div class="col-md-2">
                                            <div class="form-group">
                                                From Date
                                                @Html.TextBoxFor(m => m.FromDate, new { @class = "form-control", @readonly = "ds" })
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                To Date
                                                @Html.TextBoxFor(m => m.ToDate, new { @class = "form-control", @readonly = "ds" })
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                               Status
                                                @Html.DropDownListFor(m => m.Status, new SelectList(ViewBag.RequestStatus, "Value", "Text"), new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <br />
                                                <input class="btn btn-success" type="submit" value="Search" id="btnSearch" name="Search" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="dt-responsive table-responsive">
                                        <table id="basic-btn" class="table table-striped table-bordered nowrap">
                                            <thead>
                                                <tr>
                                                    <th>S No.</th>
                                                    <th>  Login ID</th>
                                                    <th> First Name</th>
                                                    <th>Requested Date</th>
                                                    <th>Amount</th>
                                                    <th>Status</th>
                                                    
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @if (Model != null)
                                                {
                                                    if (Model.lstPlot != null)
                                                    {
                                                        int i = 1;
                                                        foreach (var Item in Model.lstPlot)
                                                        {
                                                            <tr class="text-left">
                                                                <td>@i</td>
                                                                <td hidden>@Item.RequestID</td>
                                                                <td>@Item.AssociateLoginID</td>
                                                                <td>@Item.FirstName</td>
                                                                <td>@Item.ClosingDate  </td>
                                                                <td>@Item.GrossAmount </td>

                                                                <th><a class="@Item.DisplayName">@Item.Status</a></th>
                                                                

                                                            </tr>
                                                            i++;
                                                        }
                                                    }
                                                }

                                            </tbody>
                                            <tfoot>
                                                <tr>
                                                    <th>S No.</th>
                                                    <th>  Login ID</th>
                                                    <th> First Name</th>
                                                    <th>Requested Date</th>
                                                    <th>Amount</th>
                                                    <th>Status</th>
                                                    
                                                </tr>
                                            </tfoot>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


}



<script src="~/files/bower_components/jquery/js/jquery.min.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">
<script>

    $('#FromDate').datepicker({
        dateFormat: 'dd/mm/yy',
        changeYear: true,
        changeMonth: true,
    });
    $('#ToDate').datepicker({
        dateFormat: 'dd/mm/yy',
        changeYear: true,
        changeMonth: true,
    });

</script>






